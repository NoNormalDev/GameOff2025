code = "shader_type canvas_item;

uniform vec2 size;
uniform sampler2D tex: filter_linear, repeat_enable;
uniform float scale;
uniform float brightness;
uniform float saturation;

void vertex() {
	UV.y *= size.y/size.x;
	UV *= scale;
}

